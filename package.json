    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-jest": "^26.6.2",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "chromedriver": "^88.0.0",
    "eslint": "^7.17.0",
    "eslint-plugin-react": "^7.22.0",
    "fsevents": "^2.3.2",
    "geckodriver": "^1.22.1",
