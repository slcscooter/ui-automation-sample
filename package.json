    "selenium-webdriver": "^4.0.0-beta.1",
    "ts-jest": "^26.5.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.2",
    "weak-napi": "^2.0.2",
    "webdriver": "^7.0.7",
    "webdriver-manager": "^12.1.8",
    "webdriverio": "^7.0.9"
