{
  "name": "ui-automation-sample",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test:e2e": "yarn startup && jest -c jest.config.json ; yarn cleanup",
    "fix": "eslint --fix \"e2e/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --config .prettierrc.json --write \"e2e/**/*.{js,jsx,ts,tsx}\"",
    "clean": "bash scripts/webdriver-manager-clean.sh",
    "cleanup": "yarn shutdown ; yarn chromedriver ; yarn geckodriver ; yarn clean",
