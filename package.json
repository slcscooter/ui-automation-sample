    "geckodriver": "bash scripts/clean-up-geckodriver.sh",
    "start": "bash scripts/webdriver-manager-start.sh",
    "startup": "yarn cache clean && yarn update && yarn start",
    "shutdown": "bash scripts/webdriver-manager-shutdown.sh",
    "update": "bash scripts/webdriver-manager-update.sh",
    "test": "jest -c jest.config.json"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/runtime": "^7.13.9",
